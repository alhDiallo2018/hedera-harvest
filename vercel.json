{
    "version": 2,
    "name": "agro-sense",
    "builds": [
      {
        "src": "build/web/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/_next/static/(.*)",
        "dest": "/build/web/_next/static/$1"
      },
      {
        "src": "/static/(.*)",
        "dest": "/build/web/static/$1"
      },
      {
        "src": "/icons/(.*)",
        "dest": "/build/web/icons/$1"
      },
      {
        "src": "/assets/(.*)",
        "dest": "/build/web/assets/$1"
      },
      {
        "src": "/version.json",
        "dest": "/build/web/version.json"
      },
      {
        "src": "/manifest.json",
        "dest": "/build/web/manifest.json"
      },
      {
        "src": "/(.*)",
        "dest": "/build/web/index.html"
      }
    ]
  }